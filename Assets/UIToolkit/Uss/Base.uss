.header {
    height: 11%;
    padding-top: 0.7%;
    padding-bottom: 0.7%;
}

.screen {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    transition-duration: 0.125s;
    transition-property: opacity;
}

.init {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
}

.left_panel {
}

.panel {
    flex-basis: 20%;
    flex-grow: 1;
}

.top_panel {
    flex-direction: row-reverse;
    align-items: center;
    padding-right: 3%;
}

.title_label {
    padding-top: 5px;
    color: rgb(255, 255, 255);
}

.vertical_group {
    align-items: flex-end;
}

.mid_panel {
    padding-right: 3%;
}

.button_base {
    padding-left: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    background-color: rgba(88, 88, 88, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    color: rgb(224, 224, 224);
}

.button_base:hover {
    color: rgb(255, 255, 255);
}

.font_base {
    -unity-font-definition: url('project://database/Assets/08.Fonts/HeirofLightRegular%20SDF%204.asset?fileID=11400000&guid=90e08f45e1542c6469c7c49dd4a5e55e&type=2#HeirofLightRegular SDF 4');
}

.topleft_margin_panel {
    margin-left: 3%;
    margin-top: 3%;
}

.rightbot_align_panel {
    align-items: flex-end;
    justify-content: flex-end;
}

.rightbot_margin {
    margin-right: 3%;
    margin-bottom: 3%;
}

.center_align {
    align-items: center;
    justify-content: center;
}

.dialogue_background {
    align-items: flex-start;
    position: absolute;
    width: 100%;
    height: 100%;
    transition-duration: 0.5s;
}

.middle_size_font {
}

.quest_panel {
    width: 85%;
    height: 100%;
    background-color: rgb(29, 29, 29);
    border-top-left-radius: 30px;
    border-bottom-left-radius: 30px;
    border-top-right-radius: 30px;
    border-bottom-right-radius: 30px;
    background-image: url('project://database/Assets/14.Texture/UI/Make/Dialogue/TalkBox.png?fileID=2800000&guid=ab061285fd032474eb1f31a5b4ae9d83&type=3#TalkBox');
    -unity-background-image-tint-color: rgb(255, 255, 255);
    padding-bottom: 50px;
}

.midsize_font {
    color: rgb(255, 255, 255);
    font-size: 30px;
}

.unity-radio-button-group > .unity-text-element {
    display: none;
}

.unity-radio-button-group > .unity-base-field__input {
    display: none;
}

.unity-radio-button > .unity-label {
    display: none;
    position: absolute;
}

.unity-radio-button > .unity-base-field__input {
    display: flex;
    align-items: center;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    justify-content: center;
    width: 100%;
    height: 100%;
}

.unity-radio-button > .unity-base-field__input > .unity-radio-button__checkmark-background {
    display: flex;
    background-image: url('project://database/Packages/com.unity.timeline/Editor/StyleSheets/Images/Icons/d_TrackLockButtonEnabled@2x.png?fileID=2800000&guid=2c85066ea99cfbe4f90294bf295ce543&type=3#d_TrackLockButtonEnabled@2x');
    background-color: rgba(58, 55, 55, 0);
    -unity-background-scale-mode: scale-to-fit;
    -unity-background-image-tint-color: rgb(236, 233, 148);
    align-items: center;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    justify-content: center;
    width: 80px;
    height: 100%;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    overflow: hidden;
}

.content_panel {
    height: 90%;
    flex-direction: row;
    border-left-color: rgba(255, 255, 255, 0.48);
    border-right-color: rgba(255, 255, 255, 0.48);
    border-top-color: rgba(255, 255, 255, 0.48);
    border-bottom-color: rgba(255, 255, 255, 0.48);
    border-top-width: 0;
    border-bottom-width: 0;
    margin-left: 5%;
    margin-right: 5%;
    margin-top: 1%;
    margin-bottom: 4%;
    border-left-width: 0;
    border-right-width: 0;
}

.header_text {
    color: rgba(255, 255, 255, 0.82);
    font-size: 65px;
    -unity-font-style: bold;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 2px;
    padding-left: 20px;
}

.unity-list-view .unity-scroller {
    display: none;
}

.unity-scroll-view__content-and-vertical-scroll-container .unity-scroller {
    display: none;
}

.unity-scroll-view #unity-slider {
    display: none;
}

.unity-scroll-view #unity-low-button {
    display: none;
}

.unity-scroll-view #unity-high-button {
    display: none;
}

.frame_base {
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 1.5px;
    border-right-width: 1.5px;
    border-top-width: 1.5px;
    border-bottom-width: 1.5px;
    border-top-left-radius: 6px;
    border-bottom-left-radius: 6px;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
}

.pattern {
    background-image: url('project://database/Assets/14.Texture/UI/Make/Map/Frame.png?fileID=2800000&guid=3f1cff9bb57d3984f9bc0f1c6e258d21&type=3#Frame');
}

.title_label {
    font-size: 60px;
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    width: 370px;
    border-bottom-width: 3px;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    padding-left: 10px;
}

.popup {
    width: 500px;
    height: 300px;
    background-color: rgb(0, 0, 0);
    transform-origin: center;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
}

.radius_base {
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}
