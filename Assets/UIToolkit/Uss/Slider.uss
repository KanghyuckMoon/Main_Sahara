.unity-progress-bar .unity-progress-bar__progress {
    display: flex;
    background-color: rgb(135, 135, 135);
    justify-content: space-around;
    visibility: visible;
    height: 200%;
}

.unity-progress-bar .unity-progress-bar__background {
    display: flex;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    visibility: hidden;
    justify-content: center;
    align-items: stretch;
}

.front_bar {
    align-items: stretch;
    justify-content: flex-start;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    flex-grow: 1;
}

.unity-progress-bar .unity-progress-bar__container {
    display: flex;
    height: 100%;
    visibility: visible;
}

.hp_bar {
}

#front_bar.hp_bar.unity-progress-bar .unity-progress-bar__progress {
    display: flex;
    background-color: rgb(242, 47, 46);
    visibility: visible;
    height: 200%;
    background-image: none;
}

.skill_image {
    background-image: url('project://database/Assets/12.Texture/UI/images/icons_small.png?fileID=21300010&guid=5ca9013004c0ec84cbfaaaee55090805&type=3#icons_small_achievements');
    -unity-background-scale-mode: scale-to-fit;
    width: 80%;
    height: 80%;
}

#back_bar.hp_bar.unity-progress-bar .unity-progress-bar__progress {
    display: flex;
    background-color: rgb(125, 110, 112);
    justify-content: space-around;
    visibility: visible;
    height: 200%;
}

.mp_bar {
}

#front_bar.mp_bar.unity-progress-bar .unity-progress-bar__progress {
    display: flex;
    background-color: rgb(46, 162, 242);
    justify-content: space-around;
    visibility: visible;
    height: 200%;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

#back_bar.mp_bar.unity-progress-bar .unity-progress-bar__progress {
    display: flex;
    background-color: rgb(75, 75, 87);
    justify-content: space-around;
    visibility: visible;
    height: 200%;
}

.slider_frame {
    width: 101%;
    height: 200%;
    background-image: url('project://database/Assets/12.Texture/UI/Make/HUD/PlayerDeco.png?fileID=2800000&guid=871046fb9845e8b4891f4fdf6de4d639&type=3#PlayerDeco');
    -unity-background-scale-mode: stretch-to-fill;
    position: absolute;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

.unity-progress-bar .unity-progress-bar__title-container {
    display: none;
}
